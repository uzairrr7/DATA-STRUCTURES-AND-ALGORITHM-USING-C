#include <stdio.h>
#include <stdlib.h>

#define MAXV 10000

int V, E;
int *adj[MAXV];
int adjCount[MAXV];

int visited[MAXV];

int dfs(int u, int parent) {
    visited[u] = 1;
    for(int i = 0; i < adjCount[u]; i++) {
        int v = adj[u][i];
        if (!visited[v]) {
            if (dfs(v, u)) return 1;
        } else if (v != parent) {
            // visited and not parent => cycle
            return 1;
        }
    }
    return 0;
}

int main() {
    scanf("%d %d", &V, &E);
    for(int i=0;i<V;i++) {
        adjCount[i] = 0;
        visited[i] = 0;
    }
    for(int i=0;i<E;i++) {
        int u,v;
        scanf("%d %d", &u, &v);
        adj[u][ adjCount[u]++ ] = v;
        adj[v][ adjCount[v]++ ] = u;
    }
    int cycle = 0;
    for(int i=0;i<V;i++) {
        if (!visited[i]) {
            if (dfs(i, -1)) {
                cycle = 1;
                break;
            }
        }
    }
    printf("%d\n", cycle);
    return 0;
}
