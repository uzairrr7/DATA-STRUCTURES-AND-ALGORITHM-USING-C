#include <stdio.h>
#include <stdbool.h>

#define SIZE 1000

bool containsDuplicate(int arr[], int n) {
    int hash[SIZE] = {0};

    for(int i = 0; i < n; i++) {
        if(hash[arr[i]] == 1)
            return true;
        hash[arr[i]] = 1;
    }
    return false;
}

int main() {
    int arr[] = {1, 2, 3, 1};
    int n = 4;

    if(containsDuplicate(arr, n))
        printf("Duplicate Found\n");
    else
        printf("No Duplicate\n");
}
