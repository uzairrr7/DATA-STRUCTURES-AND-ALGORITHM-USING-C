#include <stdio.h>

union Number {
    int i;
    float f;
};

int main() {
    union Number n;

    n.i = 25;
    printf("Integer: %d\n", n.i);

    n.f = 12.5;
    printf("Float: %.2f\n", n.f);

    // Integer value is now overwritten
    printf("Integer after writing float: %d\n", n.i);

    return 0;
}
