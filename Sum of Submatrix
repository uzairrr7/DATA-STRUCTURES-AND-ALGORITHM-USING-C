#include <stdio.h>

int main() {
    int n, m, q;
    scanf("%d %d %d", &n, &m, &q);

    long long a[n + 1][m + 1];
    long long pref[n + 1][m + 1];

    // Initialize first row and column to 0 (for easier formula)
    for (int i = 0; i <= n; i++) {
        for (int j = 0; j <= m; j++) {
            a[i][j] = 0;
            pref[i][j] = 0;
        }
    }

    // Read input matrix 1-based
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            scanf("%lld", &a[i][j]);
        }
    }

    // Build 2D prefix sum
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            pref[i][j] = a[i][j] 
                         + pref[i - 1][j] 
                         + pref[i][j - 1] 
                         - pref[i - 1][j - 1];
        }
    }

    // Answer queries
    while (q--) {
        int x1, y1, x2, y2;
        scanf("%d %d %d %d", &x1, &y1, &x2, &y2);

        long long sum = pref[x2][y2]
                        - pref[x1 - 1][y2]
                        - pref[x2][y1 - 1]
                        + pref[x1 - 1][y1 - 1];

        printf("%lld\n", sum);
    }

    return 0;
}
