#include <stdio.h>

#define SIZE 1000

int main() {
    int a[] = {1, 2, 2, 3, 4};
    int b[] = {2, 2, 4, 6};
    int hash[SIZE] = {0};

    for(int i = 0; i < 5; i++)
        hash[a[i]] = 1;

    printf("Intersection: ");
    for(int i = 0; i < 4; i++) {
        if(hash[b[i]] == 1) {
            printf("%d ", b[i]);
            hash[b[i]] = 0; // avoid duplicate printing
        }
    }
}
