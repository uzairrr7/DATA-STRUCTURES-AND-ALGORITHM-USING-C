#include <stdio.h>

#define SIZE 1000

int countDistinct(int arr[], int n) {
    int hash[SIZE] = {0};
    int count = 0;

    for(int i = 0; i < n; i++) {
        if(hash[arr[i]] == 0) {
            hash[arr[i]] = 1;
            count++;
        }
    }
    return count;
}

int main() {
    int arr[] = {10, 20, 10, 30, 20, 40};
    int n = 6;

    printf("Distinct Count = %d\n", countDistinct(arr, n));
}
