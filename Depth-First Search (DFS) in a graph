#include <stdio.h>
#include <stdlib.h>
#define MAXV 50

int adj[MAXV][MAXV], V;
int visited[MAXV];

void init_graph(int v) {
    V = v;
    for(int i=0;i<V;i++)
      for(int j=0;j<V;j++)
        adj[i][j] = 0;
    for(int i=0;i<V;i++)
      visited[i] = 0;
}

void add_edge(int u, int v) {
    adj[u][v] = 1;
    adj[v][u] = 1;
}

void dfs(int u) {
    visited[u] = 1;
    printf("%d ", u);
    for(int v=0; v<V; v++) {
        if(adj[u][v] && !visited[v]) {
            dfs(v);
        }
    }
}

int main() {
    init_graph(5);
    add_edge(0,1); add_edge(0,2); add_edge(1,3); add_edge(1,4);
    printf("DFS starting from vertex 0: ");
    dfs(0);
    printf("\n");
    return 0;
}
