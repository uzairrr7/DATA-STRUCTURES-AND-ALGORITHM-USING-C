#include <stdio.h>

#define SIZE 1000

int firstRepeating(int arr[], int n) {
    int hash[SIZE] = {0};

    for(int i = 0; i < n; i++) {
        if(hash[arr[i]] == 1)
            return arr[i];
        hash[arr[i]] = 1;
    }
    return -1;
}

int main() {
    int arr[] = {5, 3, 4, 3, 5, 6};
    int n = 6;

    int ans = firstRepeating(arr, n);
    if(ans == -1)
        printf("No repeating element\n");
    else
        printf("First repeating element: %d\n", ans);
}
